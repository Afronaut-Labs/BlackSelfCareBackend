<p id="notice"><%= notice %></p>

<h1>Saved Quotes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Quote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_quotes.each do |saved_quote| %>
      <tr>
        <td><%= saved_quote.user_id %></td>
        <td><%= saved_quote.quote_id %></td>
        <td><%= link_to 'Show', saved_quote %></td>
        <td><%= link_to 'Edit', edit_saved_quote_path(saved_quote) %></td>
        <td><%= link_to 'Destroy', saved_quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved Quote', new_saved_quote_path %>
